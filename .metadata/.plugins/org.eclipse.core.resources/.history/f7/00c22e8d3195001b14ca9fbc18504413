package com.trifulcas.ejemplos;

import java.util.ArrayList;
import java.util.List;

public class Listas {

	public static void main(String[] args) {
		List<String> lista=new ArrayList<String>();
		lista.add("Eva");
		lista.add("Juan");
		lista.add(1, "Ana");
  
        System.out.println(lista);
        
        lista.set(1, "Pep");
        System.out.println(lista);
	}

}
