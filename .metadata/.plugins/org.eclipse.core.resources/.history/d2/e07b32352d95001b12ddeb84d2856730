package com.trifulcas.ejemplos;

public class CadenaBuilder {
	public static void main(String[] args) {

		StringBuilder sb = new StringBuilder("Hola ");
		sb.append("que tal");
		System.out.println(sb);
		sb.insert(1,"Java");  
		System.out.println(sb);
		sb.replace(4,6,"###");  
		System.out.println(sb);
		sb.delete(1,3);  
		System.out.println(sb);
		sb.reverse();  
		System.out.println(sb);
	}
}
