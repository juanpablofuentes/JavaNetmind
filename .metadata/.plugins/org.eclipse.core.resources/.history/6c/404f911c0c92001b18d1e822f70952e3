package com.trifulcas.clases;

import com.trifulcas.ejercitos.Ejercito;

public class ProbarClases {

	public static void main(String[] args) {
		Ejercito romanos = new Ejercito();
		
		Ejercito.aLaCarga();
		
		romanos.aLaCarga();
		romanos.retirada();
		romanos.aLaCarga();
		System.out.println(romanos.totalDanyo());
	}

}