package com.trifulcas.clases;

interface audio{
	void sonido();
}

abstract public class Vehiculos {

	public Vehiculos() {
		// TODO Auto-generated constructor stub
	}
	
	abstract void sonido();
}

class Automovil extends Vehiculos{

	@Override
	void sonido() {
		System.out.println("brum brum");	
	}
	
}

class Monociclo extends Vehiculos{

	@Override
	void sonido() {
		System.out.println("Dring dring");
		
	}
	
}