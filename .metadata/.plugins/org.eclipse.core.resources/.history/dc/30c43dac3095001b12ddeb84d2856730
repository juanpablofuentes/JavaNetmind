public class Listas {

	public static void main(String[] args) {
		 ArrayList<String> al = new ArrayList<String>();
		  
	        al.add("Geeks");
	        al.add("Geeks");
	        al.add(1, "For");
	  
	        System.out.println(al);
	}

}
