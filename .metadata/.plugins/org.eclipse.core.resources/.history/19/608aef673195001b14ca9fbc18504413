package com.trifulcas.ejemplos;

import java.util.ArrayList;
import java.util.List;

public class Listas {

	public static void main(String[] args) {
		List<String> lista=new ArrayList<String>();
		lista.add("Geeks");
		lista.add("Geeks");
		lista.add(1, "For");
  
        System.out.println(lista);
	}

}
