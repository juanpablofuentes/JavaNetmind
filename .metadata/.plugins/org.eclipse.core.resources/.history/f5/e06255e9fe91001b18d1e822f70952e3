package com.trifulcas.clases;

import com.trifulcas.ejercitos.Infanteria;

public class Ejercito {

	// No es herencia
	Infanteria[] infanteria;
	
	public Ejercito() {

		//infanteria= {new Soldado(),new Soldado()new Soldado()new Soldado()new Soldado()new Soldado(),new Lancero(),.infanteria.clone().clone().}
		
		infanteria=new Infanteria[15];
		for(int i=0;i<10;i++) {
			infanteria[i]=new Soldado();
		}
		for(int i=10;i<14;i++) {
			infanteria[i]=new Lancero();
		}
		infanteria[14]=new Capitan();
	}

}
